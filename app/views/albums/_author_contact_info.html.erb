<tr>
			<td>author name</td>
			<td><%= link_to @author['name'],user_info_path(@author['id'].to_i),:class => "read_m" %></td>
		</tr>
		<tr>
			<td>username</td>
			<td><%= @author['username'] %></td>
		</tr>
		<tr>
			<td>email</td>
			<td><%= @author['email'] %></td>
		</tr>
		<tr>
			<td>phone</td>
			<td><%= @author['phone'] %></td>
		</tr>
		<tr>
			<td>website</td>
			<td><%= @author['website'] %></td>
		</tr>
		<% @author['address'].each{|address_info| %>
			<% if address_info != nil %>
				<tr>
				<% if address_info[0] === "geo" %>
					<td><%= address_info[0] %></td><td>lat: <%= address_info[1]['lat'] %><br> lng: <%= address_info[1]['lng'] %></td>
			<% else %>
					<td><%= address_info[0] %></td><td><%= address_info[1] %></td>
				<% end %>
				</tr>
			<% end %>
		<% } %>
